<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="clinicaaudinsa.audiologia"
    android:versionCode="4"
    android:versionName="1.3" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <permission
        android:name="clinicaaudinsa.audiologia.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="clinicaaudinsa.audiologia.permission.MAPS_RECEIVE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    
     <!-- GCM connects to Internet Services. -->
    <uses-permission android:name="android.permission.INTERNET" />
 
    <!-- GCM requires a Google account. -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
 
    <!-- Keeps the processor from sleeping when a message is received. -->
    <uses-permission android:name="android.permission.WAKE_LOCK" />
 
    <!-- Creates a custom permission so only this app can receive its messages. -->
    <permission
        android:name="clinicaaudinsa.audiologia.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />
 
    <uses-permission android:name="clinicaaudinsa.audiologia.permission.C2D_MESSAGE" />
 
    <!-- This app has permission to register and receive data message. -->
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
 
    <!-- Network State Permissions to detect Internet status -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
 
    <!-- Permission to vibrate -->
    <uses-permission android:name="android.permission.VIBRATE" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="clinicaaudinsa.audiologia.PerfilesActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="clinicaaudinsa.audiologia.PerfilesMantenimientoActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_perfiles_mantenimiento" >
        </activity>
        <activity
            android:name="clinicaaudinsa.audiologia.ExamenesActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_examenes" >
        </activity>
        <activity
            android:name="clinicaaudinsa.audiologia.CuestionarioInstruccionesActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_cuestionario_instrucciones" >
        </activity>
        <activity
            android:name="clinicaaudinsa.audiologia.CuestionarioExamenActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_cuestionario_examen" >
        </activity>
        <activity
            android:name="clinicaaudinsa.audiologia.ArticulosActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_articulos" >
        </activity>
        <activity
            android:name="clinicaaudinsa.audiologia.ResultadoActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_cuestionario_resultado" >
        </activity>
        <activity
            android:name="clinicaaudinsa.audiologia.SensibilidadDeOidoInstruccionesActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_sensibilidad_oidos_instrucciones" >
        </activity>
        <activity
            android:name="clinicaaudinsa.audiologia.MapaActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_mapa_localizacion" >
        </activity>

        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyAWAs9HJnQ71bVw8GyZeAI317yWMyjJ20g" />

        <activity
            android:name="clinicaaudinsa.audiologia.AcercaDeActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_acerca_de" >
        </activity>
       
        <activity
            android:name="clinicaaudinsa.audiologia.HablaEnRuidoInstruccionesActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_habla_en_ruido_instrucciones" >
        </activity>
        <activity
            android:name="clinicaaudinsa.audiologia.SensibilidadOidoExamen"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_sensibilidad_oido_examen" >
        </activity>
        <activity
            android:name="clinicaaudinsa.audiologia.ResultadoPerfilActivity"
            android:label="@string/title_activity_resultado_perfil"
            android:screenOrientation="portrait" >
        </activity>
         <activity
            android:name="clinicaaudinsa.audiologia.MessagesActivity"
            android:label="@string/title_activity_messages" >
        </activity>
       
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/app_id" />
        
        <receiver
            android:name="com.google.android.gcm.GCMBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
 
                <!-- Receives the actual messages. -->
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <!-- Receives the registration id. -->
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />
 
                <category android:name="clinicaaudinsa.audiologia" />
            </intent-filter>
        </receiver>
 
        <service android:name=".GCMIntentService" />
    </application>

</manifest>