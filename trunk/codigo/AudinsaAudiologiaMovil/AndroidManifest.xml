<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="audinsa.audiologia"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="14" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="audinsa.audiologia.PerfilesActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="audinsa.audiologia.PerfilesMantenimientoActivity"
            android:label="@string/title_activity_perfiles_mantenimiento" >
        </activity>
        <activity
            android:name="audinsa.audiologia.ExamenesActivity"
            android:label="@string/title_activity_examenes" >
        </activity>
        <activity
            android:name="audinsa.audiologia.CuestionarioInstruccionesActivity"
            android:label="@string/title_activity_cuestionario_instrucciones" >
        </activity>
        <activity
            android:name="audinsa.audiologia.CuestionarioExamenActivity"
            android:label="@string/title_activity_cuestionario_examen" >
        </activity>
        <activity
            android:name="audinsa.audiologia.ArticulosActivity"
            android:label="@string/title_activity_articulos" >
        </activity>
        <activity
            android:name="audinsa.audiologia.CuestionarioResultadoActivity"
            android:label="@string/title_activity_cuestionario_resultado" >
        </activity>
    </application>

</manifest>